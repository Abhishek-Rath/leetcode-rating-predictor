{
    "manifest_version": 3,
    "name": "Leetcode Predictor",
    "version": "1.0.0",
    "description": "Browser extension for Leetcode rating predictions.",
    "short_name": "LCPredictor",
    "background": {
      "service_worker": "background.js"
    },
    "action":{
      "default_popup":"./popup.html",
      "default_icons":{
        
      }
    },
    "options_page":"./options.html",
    "permissions":[
      "activeTab",
      "tabs",
      "storage",
      "scripting"
    ],
    "host_permissions":[
      "<all_urls>"
    ]
   }